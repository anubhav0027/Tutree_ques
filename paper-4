#include <iostream>
using namespace std;
#include<vector>

void find(int a1[], int x, int y){
    int pos1,pos2;
    for(int i = 0; i < 10; i++){
        if(a1[i] == x){
            pos1 = i;
        }
        if(a1[i] == y){
            pos2 = i;
        }
    }
    cout << pos2-pos1 << endl;
}

int main() {
	// your code goes here
    int n;cin>> n;
	int a1[n];
	for(int i = 0; i < 10; i++){
	    cin >> a1[i] ;
	}
	int x,y; 
	cin >> x >> y;
	find(a1,x,y);
	return 0;
}
