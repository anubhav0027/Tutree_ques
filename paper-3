que - 1
#include <iostream>
using namespace std;
#include<vector>
void find(int a1[],int a2[]){
    vector<int> inter;
    vector<int> uni;
    int i = 0, j = 0;
    while(i < 10 &&  j < 10){
        if(a1[i] < a2[j]){
            i++;
            uni.push_back(a1[i]);
        }else if(a1[i] > a2[j]){
            uni.push_back(a2[j]);
            j++;
        }else {
            inter.push_back(a1[i]);
            uni.push_back(a1[i]);
            i++;j++;
        }
    }
    while(i < 10){
        uni.push_back(a1[i]);
        i++;
    }
    while(j < 10){
        uni.push_back(a2[j]);
        j++;
    }
    j = 0;
    while(j < uni.size()){
        cout << uni[j] << " ";
        j++;
    }
    cout << endl;
    i = 0;
    while(i < inter.size()){
        cout << inter[i] << " ";
        i++;
    }
    
    
}
    

int main() {
	// your code goes here
	int a1[10],a2[10];
	for(int i = 0; i < 10; i++){
	    cin >> a1[i] ;
	}
	for(int i = 0; i < 10; i++){
	    cin >> a2[i] ;
	}
	
	find(a1,a2);
	return 0;
}
