que -2 
#include <iostream>
using namespace std;
#include<vector>

void rotate(int a1[], int d, int n){
    int j = 0;
    for(int i=0; i < d; i++){
        int temp = a1[0];
        j = 0;
        while(j < n-1 ){
            a1[j] = a1[j+1]; 
            j++;
        }
        a1[j] = temp;
    }
    for(int i = 0; i < n; i++){
        cout << a1[i] << " ";
    }
}

int main() {
	// your code goes here
	int n; 
	cin >> n;
	int a1[n];
	for(int i = 0; i < 10; i++){
	    cin >> a1[i] ;
	}
	int d;
	cin >> d;
	rotate(a1,d, n);
	return 0;
}

